{
	"$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
	"type": "AdaptiveCard",
	"version": "1.0",
	"backgroundImage": "{{card.header.image}}",
	"body": [
		{
			"type": "TextBlock",
			"text": "{{#? card.header.title}}",
			"size": "extraLarge"
		},
		{
			"type": "TextBlock",
			"text": "{{#? card.header.subtitle}}",
			"size": "large"
		}
	]
}
